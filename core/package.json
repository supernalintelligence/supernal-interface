{
  "name": "@supernal-interface/core",
  "version": "1.0.0",
  "description": "Universal AI Interface - Make any application AI-controllable and auto-testable",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "demo": "cd demo && npm run dev",
    "demo:build": "cd demo && npm run build",
    "demo:start": "cd demo && npm run start",
    "test:coverage": "jest --coverage",
    "test:self": "jest tests/core.test.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "generate-tests": "ts-node src/cli/generate-tests.ts",
    "init-project": "ts-node src/cli/init.ts"
  },
  "bin": {
    "supernal-interface": "dist/cli/generate.js"
  },
  "keywords": [
    "ai",
    "interface",
    "testing",
    "automation",
    "decorators",
    "tools",
    "supernal"
  ],
  "author": "Supernal AI",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/supernal-ai/supernal-nova.git",
    "directory": "platform/services/user-management/packages/@supernal-interface/core"
  }
}
